{
  "name": "campus-optimizer-reports",
  "version": "2.0.0",
  "description": "CampusOptimizer Reporting System with Vercel Serverless Functions",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "co:compile": "node campus-optimizer/compile-data.js",
